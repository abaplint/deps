{
  "global": {
    "files": "/src/**/*.*",
    "skipGeneratedGatewayClasses": false,
    "skipGeneratedPersistentClasses": false,
    "skipGeneratedFunctionGroups": false
  },
  "syntax": {
    "version": "v702",
    "errorNamespace": "ASDF"
  },
  "rules": {
    "cds_comment_style": true,
    "no_prefixes": false,
    "reduce_procedural_code": false,
    "change_if_to_case": true,
    "no_aliases": false,
    "many_parentheses": true,
    "align_parameters": true,
    "align_type_expressions": true,
    "dynpro_checks": true,
    "prefer_pragmas": true,
    "cds_legacy_view": true,
    "cds_parser_error": true,
    "classic_exceptions_overlap": true,
    "constant_classes": true,
    "cyclic_oo": false,
    "local_testclass_consistency": true,
    "no_chained_assignment": true,
    "no_external_form_calls": true,
    "no_inline_in_optional_branches": true,
    "nrob_consistency": true,
    "omit_preceding_zeros": true,
    "pragma_style": true,
    "prefer_corresponding": true,
    "slow_parameter_passing": false,
    "static_call_via_instance": true,
    "superfluous_value": true,
    "unnecessary_chaining": false,
    "unsecure_fae": true,
    "unused_ddic": true,
    "unnecessary_pragma": true,
    "modify_only_own_db_tables": true,
    "no_yoda_conditions": true,
    "prefer_raise_exception_new": false,
    "select_add_order_by": true,
    "select_performance": true,
    "use_class_based_exceptions": false,
    "dangerous_statement": false,
    "db_operation_in_loop": false,
    "identical_descriptions": false,
    "max_one_method_parameter_per_line": true,
    "uncaught_exception": true,
    "call_transaction_authority_check": true,
    "function_module_recommendations": true,
    "identical_contents": true,
    "intf_referencing_clas": false,
    "method_implemented_twice": true,
    "method_overwrites_builtin": false,
    "omit_parameter_name": true,
    "omit_receiving": true,
    "parser_702_chaining": true,
    "prefer_is_not": true,
    "prefer_xsdbool": true,
    "sy_modification": true,
    "unused_methods": false,
    "use_bool_expression": true,
    "check_subrc": true,
    "cyclomatic_complexity": true,
    "forbidden_pseudo_and_pragma": false,
    "identical_conditions": true,
    "line_break_style": false,
    "unused_types": false,
    "use_line_exists": true,
    "begin_single_include": true,
    "downport": false,
    "line_break_multiple_parameters": false,
    "names_no_dash": true,
    "easy_to_find_messages": false,
    "expand_macros": true,
    "fully_type_itabs": false,
    "select_single_full_key": true,
    "smim_consistency": true,
    "sql_value_conversion": true,
    "strict_sql": true,
    "unnecessary_return": false,
    "parser_missing_space": true,
    "prefer_inline": false,
    "reduce_string_templates": true,
    "forbidden_void_type": false,
    "forbidden_identifier": false,
    "try_without_catch": true,
    "unknown_types": true,
    "allowed_object_naming": true,
    "keep_single_parameter_on_one_line": true,
    "prefer_returning_to_exporting": false,
    "prefix_is_current_class": true,
    "selection_screen_naming": true,
    "sicf_consistency": true,
    "sql_escape_host_variables": true,
    "unused_variables": false,
    "xml_consistency": true,
    "check_comments": true,
    "fully_type_constants": true,
    "abapdoc": false,
    "allowed_object_types": false,
    "ambiguous_statement": true,
    "begin_end_names": true,
    "chain_mainly_declarations": true,
    "check_abstract": true,
    "check_ddic": true,
    "check_include": false,
    "check_syntax": true,
    "check_text_elements": true,
    "check_transformation_exists": true,
    "form_tables_obsolete": true,
    "implement_methods": true,
    "main_file_contents": true,
    "newline_between_methods": true,
    "release_idoc": true,
    "rfc_error_handling": true,
    "type_form_parameters": true,
    "types_naming": false,
    "7bit_ascii": true,
    "avoid_use": {
      "define": true,
      "defaultKey": false,
      "break": true,
      "testSeams": true,
      "exportToDatabase": true,
      "exportToMemory": true,
      "describeLines": true,
      "statics": true
    },
    "class_attribute_names": false,
    "cloud_types": true,
    "colon_missing_space": true,
    "commented_code": true,
    "constructor_visibility_public": true,
    "contains_tab": true,
    "definitions_top": true,
    "description_empty": false,
    "double_space": {
      "keywords": true,
      "startParen": true,
      "afterColon": true,
      "endParen": true
    },
    "empty_line_in_statement": {
      "allowChained": false
    },
    "empty_statement": true,
    "empty_structure": {
      "loop": true,
      "if": true,
      "try": true,
      "loopAllowIfSubrc": false,
      "when": true,
      "while": true,
      "case": true,
      "select": true,
      "do": true,
      "at": true
    },
    "exit_or_check": true,
    "exporting": true,
    "functional_writing": {
      "ignoreExceptions": true
    },
    "global_class": true,
    "identical_form_names": true,
    "if_in_if": true,
    "in_statement_indentation": {
      "blockStatements": 0,
      "ignoreExceptions": true
    },
    "indentation": {
      "ignoreExceptions": true,
      "alignTryCatch": false,
      "globalClassSkipFirst": false,
      "ignoreGlobalClassDefinition": false,
      "selectionScreenBlockIndentation": false,
      "ignoreGlobalInterface": false
    },
    "inline_data_old_versions": true,
    "keyword_case": {
      "style": "upper",
      "ignoreExceptions": true,
      "ignoreLowerClassImplmentationStatement": true,
      "ignoreGlobalClassBoundaries": false,
      "ignoreGlobalClassDefinition": false,
      "ignoreGlobalInterface": false,
      "ignoreFunctionModuleName": false,
      "ignoreKeywords": []
    },
    "line_length": {
      "length": 120
    },
    "line_only_punc": {
      "ignoreExceptions": true
    },
    "local_class_naming": {
      "local": "^LCL_.*$",
      "test": "^LTCL_.*$",
      "exception": "^LCX_.*"
    },
    "local_variable_names": {
      "expectedData": "^L._.*$",
      "expectedConstant": "^LC_.*$",
      "expectedFS": "^<L._.*>$"
    },
    "max_one_statement": true,
    "message_exists": true,
    "method_length": {
      "errorWhenEmpty": true,
      "checkForms": true,
      "ignoreTestClasses": false,
      "statements": 100
    },
    "method_parameter_names": false,
    "mix_returning": false,
    "msag_consistency": true,
    "nesting": {
      "depth": 5
    },
    "no_public_attributes": false,
    "object_naming": false,
    "obsolete_statement": {
      "fieldSymbolStructure": true,
      "refresh": true,
      "formDefinition": true,
      "formImplementation": true,
      "compute": true,
      "clientSpecified": true,
      "exitFromSQL": true,
      "regex": true,
      "occurences": true,
      "sortByFS": true,
      "callTransformation": true,
      "freeMemory": true,
      "selectWithoutInto": true,
      "add": true,
      "withHeaderLine": false,
      "subtract": true,
      "communication": true,
      "pack": true,
      "parameter": true,
      "ranges": true,
      "load": true,
      "setExtended": true,
      "multiply": true,
      "move": true,
      "typePools": true,
      "divide": true,
      "requested": true,
      "occurs": true
    },
    "parser_error": true,
    "preferred_compare_operator": true,
    "remove_descriptions": {
      "ignoreWorkflow": false,
      "ignoreExceptions": false
    },
    "sequential_blank": {
      "lines": 4
    },
    "short_case": {
      "length": 1,
      "allow": []
    },
    "space_before_colon": true,
    "space_before_dot": {
      "ignoreGlobalDefinition": false,
      "ignoreExceptions": true
    },
    "start_at_tab": true,
    "superclass_final": true,
    "tabl_enhancement_category": true,
    "unreachable_code": true,
    "use_new": true,
    "when_others_last": true,
    "whitespace_end": true
  }
}